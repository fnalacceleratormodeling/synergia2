! pr=10, subroutine commands.

do:           keyword, pr=10, sp=1, &
              " "      = (i(1)), &          space for repeat count
              times    = (i(1), =1)

enddo:        keyword, pr=10, sp=2

subroutine:   keyword, pr=10, sp=5, &
              " "      = (i(1)) !           space for repeat count

endsubroutine:keyword, pr=10, sp=6

callsubroutine:keyword, pr=10, sp=7, &
              " "      = (n(1), =" ")

if:           keyword, pr=10, sp=8, &
              " "      = (i(1)), &    space for repeat count
              cond     = (d(1), =0), &
              op       = (n(1), =">"), &
              value    = (d(1), =0)

endif:        keyword, pr=10, sp=9


! pr=11, miscellaneous service and control commands.

packmemory:   keyword, pr=11, sp=1

option:       keyword, pr=11, sp=2, &
              reset    = (l(1), =.true.), &
              tell     = (l(1), =.true.), &
              cofact   = (r(1), =1.0), &
              command  = (i(1), =1), &
              define   = (i(1), =1), &
              express  = (i(1), =1), &
              keyword  = (i(1), =1), &
              line     = (i(1), =1), &
              debug    = (l(1), =.true.), &
              double   = (l(1), =.true.), &
              echo     = (l(1), =.true.), &
              inter    = (l(1), =.true.), &
              trace    = (l(1), =.true.), &
              verify   = (l(1), =.true.), &
              warn     = (l(1), =.true.), &
              info     = (l(1), =.true.), &
              sympl    = (l(1), =.true.), &
              rbarc    = (l(1), =.true.), &
              ereset   = (l(1), =.true.), &
              bborbit  = (l(1), =.true.)


setopts:      keyedit, option

stop:         keyword, pr=11, sp=3

set:          keyword, pr=11, sp=4, &
              variable = (v(1)), &
              value    = (r(1), =0.0)

value:        keyword, pr=11, sp=5, &
              value    = (r(10))

end_file:     keyword, pr=11, sp=6

push:         keyword, pr=11, sp=7, &
              value    = (r(100))

endpush:      keyword, pr=11, sp=8, &
              save     = (n(1), ="special")



! pr=12, i/o commands.

archive:      keyword, pr=12, sp=1, &
              table    = (n(1), ="TWISS"), &
              filename = (s(1), ="twiss")

retrieve:     keyedit, archive, sp=2

assign:       keyword, pr=12, sp=3, &
              data     = (s(1), =""), &
              echo     = (s(1), =""), &
              print    = (s(1), ="")

call:         keyword, pr=12, sp=4, &
              filename = (s(1), ="save")

return:       keyword, pr=12, sp=5

excite:       keyword, pr=12, sp=6, &
              filename = (s(1), =" "), &
              name     = (n(1), ="name"), &
              attribute= (n(1), ="attribute"), &
              value    = (n(1), ="value")

increment:    keyedit, excite, sp=7, &
              factor   = (r(1), =1.0)

pooldump:     keyword, pr=12, sp=8, &
              filename = (s(1), ="pooldump")

poolload:     keyedit, pooldump, sp=9

save:         keyword, pr=12, sp=10, &
              filename = (s(1), ="save"), &
              pattern  = (s(1), =" ")

status:       keyword, pr=12, sp=11

system:       keyword, pr=12, sp=12, &
              command  = (s(1), =" ")

help:         keyword, pr=12, sp=13, &
              " "      = (n(1), =" ")

show:         keyword, pr=12, sp=14, &
              " "      = (n(1), =" ")

table:        keyword, pr=12, sp=15, &
              name     = (n(1), =" "), &
              columns  = (n(20), =" "), &
              sum      = (n(20), =" ")

saveline:     keyword, pr=12, sp=16, &
              name     = (n(1), = " "), &
              filename = (s(1), = "line")

