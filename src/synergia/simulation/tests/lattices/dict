8.51
! MAD dictionary
! (from /afs/cern.ch/Hans.Grote/src/mad_new/madpackage.tgz on 26-OCT-2004)

!-------------------------------------------------------------------------------
! Modified: 29-OCT-2004, M. Woodley (SLAC)
!   Correct attribute types in MATCH and CONSTRAINT definitions; add CSRDRIFT
!   element (same as DRIFT) for XTFF-to-ELEGANT compatibility; added RTAPE
!   option to TWISS command; make default TILT values positive for SBEND,
!   RBEND, GBEND, QUADRUPOLE, and SEXTUPOLE (why were they negative? something
!   to do with the "bv" flag?) 
!-------------------------------------------------------------------------------

! pr=2, parameters and related commands.

constant:     keyword, pr=2, sp=1, &
              value    = (r(1), =0.0)

parameter:    keyword, pr=2, sp=2, &
              value    = (r(1), =0.0)

! standard constants.

pi:           constant = 3.1415926535898
twopi:        constant = 2 * pi
degrad:       constant = 180 / pi
raddeg:       constant = pi / 180
e:            constant = 2.7182818284590
emass:        constant = 0.51099906e-3
pmass:        constant = 0.93827231
clight:       constant = 299792458.

! pr=3, strings and table-specific expressions.

string:       keyword, pr=3, sp=1, &
              value    = (s(3), =3\*"")

t_eigen:      keyword, pr=3, sp=2, &
              tilt1    = (s(3), =3\*""), &
              tilt2    = (s(3), =3\*""), &
              eccn1    = (s(3), =3\*""), &
              eccn2    = (s(3), =3\*"")

t_eigen: t_eigen, &
              tilt1    = &
         "0.5*ATAN(2*(E11*E31+E12*E32)/(E11^2+E12^2-E31^2-E32^2))*180/PI", &
                         "deg", "TILT1", &
              tilt2    = &
         "0.5*ATAN(2*(E13*E33+E14*E34)/(E13^2+E14^2-E33^2-E34^2))*180/PI", &
                         "deg", "TILT2", &
              eccn1    = &
"SQRT((E11^2+E12^2+E31^2+E32^2-SQRT((E11^2+E12^2)^2+(E31^2+E32^2)^2+
2*(E11^2-E12^2)*(E31^2-E32^2)+8*E11*E12*E31*E32))/
(E11^2+E12^2+E31^2+E32^2+SQRT((E11^2+E12^2)^2+(E31^2+E32^2)^2+
2*(E11^2-E12^2)*(E31^2-E32^2)+8*E11*E12*E31*E32)))", &
                         " ", "ECCN1", &
              eccn2    = &
"SQRT((E13^2+E14^2+E33^2+E34^2-SQRT((E13^2+E14^2)^2+(E33^2+E34^2)^2+
2*(E13^2-E14^2)*(E33^2-E34^2)+8*E13*E14*E33*E34))/
(E13^2+E14^2+E33^2+E34^2+SQRT((E13^2+E14^2)^2+(E33^2+E34^2)^2+
2*(E13^2-E14^2)*(E33^2-E34^2)+8*E13*E14*E33*E34)))", &
                         " ", "ECCN2"

t_track:      keyword, pr=3, sp=2, &
              distance = (s(3), =3\*""), &
              lyapunov = (s(3), =3\*""), &
              logdist  = (s(3), =3\*""), &
              logturns = (s(3), =3\*"")

t_track:  t_track, &
              distance = &
              "SQRT(X^2+PX^2+Y^2+PY^2+T^2+PT^2)/D0", &
              lyapunov = &
              "0.5*LOG((X^2+PX^2+Y^2+PY^2+T^2+PT^2)/D0^2)/TURNS", &
              logdist  = &
              "0.5*LOG((X^2+PX^2+Y^2+PY^2+T^2+PT^2)/D0^2)", &
              logturns = "LOG(TURNS)"


! pr=5, physical element definitions.

drift:        keyword, pr=5, sp=1, &
              type     = (n(1), ='DRIFT'), &
              l        = (r(1), =0.0)

rbend:        keyword, pr=5, sp=2, &
              type     = (n(1), ='RBEND'), &
              l        = (r(1), =0.0), &
              angle    = (r(1), =0.0), &
              k1       = (r(1), =0.0), &
              e1       = (r(1), =0.0), &
              e2       = (r(1), =0.0), &
              tilt     = (r(1), =pi/2), &
              k2       = (r(1), =0.0), &
              h1       = (r(1), =0.0), &
              h2       = (r(1), =0.0), &
              hgap     = (r(1), =0.0), &
              fint     = (r(1), =0.5), &
              fintx    = (r(1), =-1.0), &  ! negative value sets fintx = fint
              k3       = (r(1), =0.0), &
              fcsr     = (r(1), =0.0), &
              bv       = (l(1), =.true.), &
              gmin     = (r(1), =0.0), &
              gmax     = (r(1), =0.0)

