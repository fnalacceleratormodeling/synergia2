              h2       = (r(1), =0.0), &
              hgap     = (r(1), =0.0), &
              fint     = (r(1), =0.5), &
              fintx    = (r(1), =-1.0), &  ! negative value sets fintx = fint
              k3       = (r(1), =0.0), &
              fcsr     = (r(1), =0.0), &
              bv       = (l(1), =.true.), &
