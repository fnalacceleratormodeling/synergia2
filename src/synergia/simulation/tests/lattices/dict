              particle = (n(1), ="POSITRON"), &
              sequence = (n(1), =" "), &
              mass     = (r(1), =emass), &
              charge   = (r(1), =1.0), &
              energy   = (r(1), =1.0), &
              pc       = (r(1), =0.0), &
              gamma    = (r(1), =0.0), &
              ex       = (r(1), =1.0), &
              exn      = (r(1), =0.0), &
